{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pound": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pound",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@mdi/font/css/materialdesignicons.min.css",
              "node_modules/simplebar/dist/simplebar.min.css",
              "node_modules/@iconscout/unicons/css/line.css",
              "node_modules/tobii/dist/css/tobii.min.css",
              "node_modules/flag-icons/css/flag-icons.min.css",
              "node_modules/swiper/swiper.min.css",
              "src/assets/css/style.min.css",
              "src/assets/css/style.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/simplebar/dist/simplebar.min.js",
              "node_modules/jarallax/dist/jarallax.min.js",
              "node_modules/tobii/dist/js/tobii.min.js",
              "node_modules/swiper/swiper.min.js",
              "src/assets/js/plugins.init.js",
              "src/assets/js/app.js",
              "src/assets/js/checkout.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500mb",
                  "maximumError": "500mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "500mb",
                  "maximumError": "500mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "pound:build:production"
            },
            "development": {
              "browserTarget": "pound:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pound:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
          "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@mdi/font/css/materialdesignicons.min.css",
              "node_modules/simplebar/dist/simplebar.min.css",
              "node_modules/@iconscout/unicons/css/line.css",
              "node_modules/tobii/dist/css/tobii.min.css",
              "node_modules/flag-icons/css/flag-icons.min.css",
              "node_modules/swiper/swiper.min.css",
              "src/assets/css/style.min.css",
              "src/assets/css/style.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/simplebar/dist/simplebar.min.js",
              "node_modules/jarallax/dist/jarallax.min.js",
              "node_modules/tobii/dist/js/tobii.min.js",
              "node_modules/swiper/swiper.min.js",
              "src/assets/js/plugins.init.js",
              "src/assets/js/app.js",
              "src/assets/js/checkout.js"
            ]
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
